@{
    Layout = "";
}
<script src="../../Scripts/Cuisine/Cuisine.js" type="text/javascript"></script>
@model IEnumerable<Cuisine.Models.Category>

<table>
@foreach (var item in Model) {
    <tr>
        <td>
            <a href="@Url.Action("ListByCategory", "Product", new { id = item.CategoryId })"> @Html.DisplayFor(modelItem => item.Name)</a>
        </td>
    </tr>
}
</table>
