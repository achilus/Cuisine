@{
    ViewBag.Title = "Contact";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false"></script> 
<script src="../../Scripts/jquery-1.7.1.min.js" type="text/javascript"></script>
<script src="../../Scripts/jquery.gomap-1.3.2.min.js" type="text/javascript"></script>
<script type="text/javascript">
    $(function () {
        $("#map").goMap({
            markers: [{
                latitude: 51.3661,
                longitude: -0.100443,                
                html: {
                    content: 'South Bridge Charcoal Grill !',
                    popup: true
                }                          
            }],
            hideByClick: true,
            zoom:15
           
        });
    });
    $("#liContact").addClass('active');

</script>

<h2>Contact Information</h2>
<div id="wrapper" style="width: 800px">
    <div id="contentbottomright" style="float: right">
    <span>Opening hours <br/>
    SUNDAY-WEDNESDAY : 12.00-12.00
    THURSDAY : 12.00-12.30
    FRIDAY/SATURDAY : 12 TILL LATE
    7 DAYS A WEEK
    </span> 
    
    
    <span>020 8681 1566</span> 

</div>
